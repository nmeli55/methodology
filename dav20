import acm.program.ConsoleProgram;
//20. კონსოლიდან წაიკითხეთ n, შემდეგ წაიკითხეთ n ცალი მთელი რიცხვი და დაბეჭდეთ ჯამი
public class davalebebi extends ConsoleProgram{
	public void run() {
		int count = readInt("Enter the count of numbers: ");
		int sum = 0;
		for(int i = 0; i < count; i++){
			sum += readInt("Enter number: ");
		}
		println(" the sum of numbers is: " + sum);
	}
}
